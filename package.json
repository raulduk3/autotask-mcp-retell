{
	"name": "@layer7systems/autotask-mcp-retell",
	"version": "0.1.0",
	"description": "MCP server for Autotask ticket creation via Retell AI",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "tsc",
		"dev": "node --loader ts-node/esm src/server.ts",
		"start": "node dist/server.js",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.25.1",
		"body-parser": "^2.2.1",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.3",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.2",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.7.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	}
}
